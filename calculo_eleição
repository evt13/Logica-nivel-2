print("DIGITE O numero de votos nulos")
votos_nulos= int(input())
print("DIGITE O NUMERO de votos em branco")
votos_em_branco= int(input())
print("DIGITE O NUMERO de votos validos")
votos_validos= int(input())
total_de_eleitores=votos_nulos+votos_em_branco+votos_validos
print(total_de_eleitores)
percentual_de_votos_nulos= (votos_nulos/total_de_eleitores*100)
print(percentual_de_votos_nulos,"%")
percentual_de_votos_em_branco=  (votos_em_branco/total_de_eleitores*100)
print(percentual_de_votos_em_branco,"%")



#terminar
print("DIGITE O numero de votos nulos")
votos_nulos= int(input())
print("DIGITE O NUMERO de votos em branco")
votos_em_branco= int(input())
print("DIGITE O NUMERO de votos validos")
votos_validos= int(input())
total_de_eleitores=votos_nulos+votos_em_branco+votos_validos
print("total de eleitores",total_de_eleitores)
percentual_de_votos_nulos= (votos_nulos/total_de_eleitores*100)
print("votos nulos"): {percentual_de_votos_nulos:.3f}("%")
percentual_de_votos_em_branco=  (votos_em_branco/total_de_eleitores*100)
print("votos em branco"): {percentual_de_votos_em_branco:.3f}("%")
percentual_de_votos_validos= (votos_validos/total_de_eleitores*100)
print("votos validos"): {percentual_de_votos_validos:.3f}("%")
